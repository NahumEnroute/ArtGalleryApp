<div id="<%= dom_id artwork %>">
  <p>
    <strong>User:</strong>
    <%= "#{artwork.user_id} - #{artwork.user.first_name} #{artwork.user.last_name}" %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= artwork.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= artwork.description %>
  </p>

  <p>
    <strong>Movement:</strong>
    <%= artwork.movement %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= artwork.price %>
  </p>

</div>
